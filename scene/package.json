{
  "name": "Meta GamiMall",
  "version": "1.0.0",
  "description": "MetaGamiMall is a Metaverse Gamifi-Mall for games, exhibitions & social. Meta GamiMall is operated by MetaLiveStudio and MetaDoge",
  "navmapThumbnail": "images/scene-thumbnail.png",
  "scripts": {
    "start": "sdk-commands start",
    "deploy": "sdk-commands deploy",
    "build": "sdk-commands build",
    "upgrade-sdk": "npm install --save-dev @dcl/sdk@latest",
    "upgrade-devDependencies": "npm install --save-dev @dcl/sdk@latest @dcl/asset-packs@latest @dcl/js-runtime@latest",
    "upgrade-sdk:next": "npm install --save-dev @dcl/sdk@next",
    "pack": "sdk-commands pack-smart-wearable"
  },
  "devDependencies": {
    "@dcl/asset-packs": "^1.20.0",
    "@dcl/js-runtime": "7.5.4",
    "@dcl/sdk": "7.5.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "none"
  },
  "dependencies": {
    "@dcl-sdk/utils": "^1.2.8",
    "colyseus.js": "^0.14.15-alpha.0",
    "dcl-ui-toolkit": "0.9.1",
    "eth-connect": "^6.2.1"
  },
  "bundledDependencies": [
    "colyseus.js",
    "dcl-ui-toolkit",
    "eth-connect",
    "vlm-dcl"
  ]
}
